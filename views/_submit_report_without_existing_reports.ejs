<div class="side-content" id="getOnMapFirst" tabindex="-1" role="dialog" aria-labelledby="getOnMapFirstForm" aria-hidden="true">
  <div>
    <div>
      <form role="form" id="getOnMapFirstForm">
        <div class="row">
          <h4 id="myFormLabel">No reports for</h4>
          <p data-bind="text: selectedAddress().formatted_address"></p>
        </div>
        <div>
          <div class="form-group">
            <span>Be the first to report NO recycling in your building</span>
          </div>
        </div>
        <div class='form-group'>
          <label class='control-label'>Comment (optional)</label><br>
          <textarea data-bind="value:comment" class='form-control' name='comment' id='comment'></textarea>
        </div>
        <input type="submit" value="Report my building!" class="btn btn-primary" data-bind='click:save'>
      </form>
    </div>
  </div>
</div>
