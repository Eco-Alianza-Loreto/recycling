<form role="form" id="searchForm">
  <div>
    <h2 id="searchFormLabel">No recycling in your building?</h2>
    <h3>Report it!</h3>
  </div>
  <div>
    <div class="form-group">
      <label for="address">Enter your Chicago street address:</label>
      <input name="address" type="text" class="form-control" id="inputAddress" placeholder="Address" />
    </div>
  </div>
  <div>
    <input type="submit" value="Find my building" class="btn btn-primary" data-bind='click:search'>
  </div>
</form>
